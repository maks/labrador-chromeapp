{
	"name": "Chrome App Scaffold",
	"version": "0.0.1",
	"manifest_version": 2,
	"minimum_chrome_version": "39",
	"description": "A Chrome Packaged App Scaffold",
	"icons": {
		"16": "assets/icon-16.png",
		"128": "assets/icon-128.png"
	},
	"app": {
      "background": {
        "scripts": ["background.js"]
      }
    },
	"offline_enabled": true,
	"permissions": [
	  "storage",
	  "usb"
	],
	"optional_permissions": [
		{
		    "usbDevices": [
		    {
		       "vendorId": 1003,
		       "productId": 40960
		    }
	       ]
	   }
	 ] 
}
